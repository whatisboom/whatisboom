{
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "comma-dangle": 2,
    "no-cond-assign": 2,
    "no-console": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-unreachable": 2,
    "valid-typeof": 2,
    "curly": [2,"all"],
    "default-case": 2,
    "dot-notation": 2,
    "dot-location": 2,
    "eqeqeq": 2,
    "no-loop-func": 2,
    "no-param-reassign": 2,
    "no-redeclare": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "brace-style": [2,"1tbs",{}],
    "eol-last": 2,
    "linebreak-style": [2,"unix"],
    "new-cap": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-nested-ternary": 2,
    "no-trailing-spaces": 2,
    "no-underscore-dangle": 2,
    "no-unneeded-ternary": 2,
    "keyword-spacing": [2,{"before": true,"after": true}],
    "space-before-blocks": [2,"always"],
    "space-before-function-paren": ["error","never"],
    "spaced-comment": [2,"always",{}],
    "arrow-spacing": [2,{"before":true,"after":true}],
    "constructor-super": 2,
    "no-var": 2,
    "prefer-const": 2,
    "max-len": [2,120,{"ignoreComments":false,"ignoreUrls":true}],
    "max-params": [2,3]
  }
}
